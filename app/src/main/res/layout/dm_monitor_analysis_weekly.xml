<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="@color/sky_blue" >










    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:id="@+id/relativeLayout53" >

        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight=".42"
            android:paddingBottom="5dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

        <TextView
            android:id="@+id/bloodsugar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text = "Blood Sugar"
            android:paddingTop="5dp"
            android:paddingLeft="5dp"
            android:textColor="@color/colorPrimary"
            fontPath="Roboto-Regular.ttf"
            android:textSize="@dimen/custom_font_Level_one"
             />
            <TextView
                android:id="@+id/bs_maxunit"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text = "(mg/dl)"

                android:paddingRight="5dp"
                android:textColor="@color/colorPrimary"
                fontPath="Roboto-Regular.ttf"
                android:textSize="@dimen/custom_font_Level_two"

                />
</LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="0.15"
            android:gravity="right"
            android:padding="5dp"
            android:layout_height="wrap_content"
            android:orientation="vertical">
        <TextView
            android:id="@+id/bs_max"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text = "72"
            android:textColor="@color/colorPrimary"
            fontPath="Roboto-Regular.ttf"
            android:textSize="@dimen/custom_font_Level_one"

           />



</LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="0.15"
            android:gravity="right"
            android:padding="5dp"
            android:layout_height="wrap_content"
            android:orientation="vertical">
        <TextView
            android:id="@+id/bs_min"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text = "bs_mn"
            android:gravity="right"
            fontPath="Roboto-Regular.ttf"
            android:textColor="@color/colorPrimary"

            android:textSize="@dimen/custom_font_Level_one"
          />
            <TextView
                android:id="@+id/bs_minunit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text = "bs_mn"
                android:paddingLeft="5dp"
                android:visibility="gone"
                android:paddingRight="5dp"
                fontPath="Roboto-Regular.ttf"
                android:textColor="@color/colorPrimary"
                android:textSize="@dimen/custom_font_Level_two"
                />
</LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="0.2"
            android:gravity="right"
            android:padding="5dp"
            android:layout_height="wrap_content"
            android:orientation="vertical">
        <TextView
            android:id="@+id/avg_bs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text = "sy_ag"
            android:gravity="right"
            android:textColor="@color/colorPrimary"
            fontPath="Roboto-Regular.ttf"
            android:textSize="@dimen/custom_font_Level_one"

            />
            <TextView
                android:id="@+id/avg_bsunit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text = "sy_ag"
                android:visibility="gone"
                android:textColor="@color/colorPrimary"
                fontPath="Roboto-Regular.ttf"
                android:textSize="@dimen/custom_font_Level_two"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                />
</LinearLayout>


        <TextView

            android:layout_width="15dp"
        android:layout_height="wrap_content"
        android:layout_weight="0.07"
        />



    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:background="@color/gray"
        android:layout_height="1px"/>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
       >
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight=".42"
            android:paddingBottom="5dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

        <TextView
            android:id="@+id/weight"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="5dp"
            android:paddingLeft="5dp"
            android:textColor="@color/colorPrimary"
            fontPath="Roboto-Regular.ttf"
            android:textSize="@dimen/custom_font_Level_one"

            android:text = "Weight" />
            <TextView
                android:id="@+id/max_wtunit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text = "(wt_mx)"
                fontPath="Roboto-Regular.ttf"
                android:textSize="@dimen/custom_font_Level_two"
                android:paddingRight="5dp"
                android:textColor="@color/colorPrimary"

                />
</LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="0.15"
            android:gravity="right"
            android:padding="5dp"
            android:layout_height="wrap_content"
            android:orientation="vertical">
        <TextView
            android:id="@+id/max_wt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text = "wt_mx"
            android:gravity="right"
            fontPath="Roboto-Regular.ttf"
            android:textSize="@dimen/custom_font_Level_one"

            android:textColor="@color/colorPrimary"

           />

            </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="0.15"
            android:gravity="right"
            android:padding="5dp"
            android:layout_height="wrap_content"
            android:orientation="vertical">
        <TextView
            android:id="@+id/min_wt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text = "wt_mn"
            android:gravity="left"
            fontPath="Roboto-Regular.ttf"
            android:textSize="@dimen/custom_font_Level_one"

            android:textColor="@color/colorPrimary"

             />
            <TextView
                android:id="@+id/min_wtunit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text = "wt_mn"
                android:visibility="gone"
                fontPath="Roboto-Regular.ttf"
                android:textSize="@dimen/custom_font_Level_two"
                android:textColor="@color/colorPrimary"

                />
</LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="0.2"
            android:gravity="right"
            android:padding="5dp"
            android:layout_height="wrap_content"
            android:orientation="vertical">
        <TextView
            android:id="@+id/avg_wt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text = "avg_wt"
            fontPath="Roboto-Regular.ttf"
            android:textSize="@dimen/custom_font_Level_one"
            android:gravity="right"

            android:textColor="@color/colorPrimary"
            />
            <TextView
                android:id="@+id/avg_wtunit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text = "avg_wt"
                android:visibility="gone"
                fontPath="Roboto-Regular.ttf"
                android:textSize="@dimen/custom_font_Level_two"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:textColor="@color/colorPrimary"
                android:drawablePadding="5dp"
                />
</LinearLayout>
        <TextView

            android:layout_width="15dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.07"
            />


    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:background="@color/gray"
        android:layout_height="1px"/>

</LinearLayout>
