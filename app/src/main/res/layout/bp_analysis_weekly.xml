<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:tools="http://schemas.android.com/tools"
              android:layout_width="fill_parent"
              android:layout_height="wrap_content"
              android:background="@color/sky_blue"
              android:orientation="vertical"
              tools:ignore="MissingPrefix">


    <LinearLayout
        android:id="@+id/relativeLayout53"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/systolic"
            fontPath="Roboto-Regular.ttf"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight=".4"
            android:drawablePadding="5dp"
            android:padding="5dp"
            android:text="Systolic"
            android:textColor="@color/colorPrimary"
            android:textSize="@dimen/custom_font_Level_one"/>

        <!-- <TextView
             android:id="@+id/dia_max"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:text = "da_mx"
             android:textSize="15dp"
             android:layout_alignBaseline="@+id/diastolic"
             android:layout_alignBottom="@+id/diastolic"
             android:layout_alignRight="@+id/sys_max"
             android:layout_alignEnd="@+id/sys_max" />-->

        <TextView
            android:id="@+id/sys_max"
            fontPath="Roboto-Regular.ttf"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.15"
            android:drawablePadding="5dp"
            android:gravity="right"
            android:padding="5dp"
            android:text="sy_mx"
            android:textColor="@color/orange"
            android:textSize="@dimen/custom_font_Level_one"/>


        <TextView
            android:id="@+id/sys_min"
            fontPath="Roboto-Regular.ttf"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.15"
            android:drawablePadding="5dp"
            android:gravity="right"
            android:padding="5dp"
            android:text="sy_mn"
            android:textColor="@color/orange"
            android:textSize="@dimen/custom_font_Level_one"/>

        <TextView
            android:id="@+id/avg_sys"
            fontPath="Roboto-Regular.ttf"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.2"
            android:drawablePadding="5dp"
            android:gravity="right"
            android:padding="5dp"
            android:text="sy_ag"
            android:textColor="@color/orange"
            android:textSize="@dimen/custom_font_Level_one"/>


        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.1"
            />


    </LinearLayout>

    <View
        android:layout_width="match_parent"


        android:layout_height="1px"
        android:background="@color/gray"/>

    <LinearLayout
        android:id="@+id/relativeLayout54"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >

        <TextView
            android:id="@+id/diastolic"
            fontPath="Roboto-Regular.ttf"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.4"
            android:drawablePadding="5dp"
            android:padding="5dp"
            android:text="Diastolic"
            android:textColor="@color/colorPrimary"
            android:textSize="@dimen/custom_font_Level_one"/>


        <TextView
            android:id="@+id/dia_max"
            fontPath="Roboto-Regular.ttf"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.15"
            android:drawablePadding="5dp"
            android:gravity="right"
            android:padding="5dp"
            android:text="da_mx"
            android:textColor="@color/orange"
            android:textSize="@dimen/custom_font_Level_one"
            tools:Text="dia_min"/>

        <TextView
            android:id="@+id/dia_min"
            fontPath="Roboto-Regular.ttf"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.15"
            android:drawablePadding="5dp"
            android:gravity="right"
            android:padding="5dp"
            android:text="da_mn"
            android:textColor="@color/orange"
            android:textSize="@dimen/custom_font_Level_one"
            tools:Text="dia_min"/>


        <TextView
            android:id="@+id/avg_dia"
            fontPath="Roboto-Regular.ttf"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.2"
            android:drawablePadding="5dp"
            android:gravity="right"
            android:padding="5dp"
            android:text="da_ag"
            android:textColor="@color/orange"
            android:textSize="@dimen/custom_font_Level_one"/>

        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.1"
            />

    </LinearLayout>

    <View
        android:layout_width="match_parent"

        android:layout_height="1px"
        android:background="@color/gray"/>

    <LinearLayout
        android:id="@+id/relativeLayout55"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >

        <TextView
            android:id="@+id/pulse"
            fontPath="Roboto-Regular.ttf"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.4"
            android:drawablePadding="5dp"
            android:padding="5dp"
            android:text="Pulse"
            android:textColor="@color/colorPrimary"
            android:textSize="@dimen/custom_font_Level_one"/>


        <TextView
            android:id="@+id/max_pulse"
            fontPath="Roboto-Regular.ttf"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.15"
            android:drawablePadding="5dp"
            android:gravity="right"
            android:padding="5dp"
            android:text="pu_mx"
            android:textColor="@color/orange"
            android:textSize="@dimen/custom_font_Level_one"/>

        <TextView
            android:id="@+id/min_pulse"
            fontPath="Roboto-Regular.ttf"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.15"
            android:drawablePadding="5dp"
            android:gravity="right"
            android:padding="5dp"
            android:text="pu_mn"
            android:textColor="@color/orange"
            android:textSize="@dimen/custom_font_Level_one"/>


        <TextView
            android:id="@+id/avg_pulse"
            fontPath="Roboto-Regular.ttf"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.2"
            android:drawablePadding="5dp"
            android:gravity="right"
            android:padding="5dp"
            android:text="pu_ag"
            android:textColor="@color/orange"
            android:textSize="@dimen/custom_font_Level_one"
            />

        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.1"
            />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/gray"/>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >

        <TextView
            android:id="@+id/weight"
            fontPath="Roboto-Regular.ttf"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.4"
            android:drawablePadding="5dp"
            android:padding="5dp"
            android:text="Weight"
            android:textColor="@color/colorPrimary"
            android:textSize="@dimen/custom_font_Level_one"/>


        <TextView
            android:id="@+id/max_wt"
            fontPath="Roboto-Regular.ttf"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.15"
            android:drawablePadding="5dp"
            android:gravity="right"
            android:padding="5dp"
            android:text="wt_mx"
            android:textColor="@color/orange"
            android:textSize="@dimen/custom_font_Level_one"/>

        <TextView
            android:id="@+id/min_wt"
            fontPath="Roboto-Regular.ttf"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.15"
            android:drawablePadding="5dp"
            android:gravity="right"
            android:padding="5dp"
            android:text="wt_mn"
            android:textColor="@color/orange"
            android:textSize="@dimen/custom_font_Level_one"/>


        <TextView
            android:id="@+id/avg_wt"
            fontPath="Roboto-Regular.ttf"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.2"
            android:drawablePadding="5dp"
            android:gravity="right"
            android:padding="5dp"
            android:text="avg_wt"
            android:textColor="@color/orange"
            android:textSize="@dimen/custom_font_Level_one"/>

        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.1"
            />


    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/gray"/>

</LinearLayout>
